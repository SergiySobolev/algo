<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui">

    <ui:with field='res' type="com.sbk.algo.client.resources.AlgoResources"/>
    <ui:with field='i18n' type='com.sbk.algo.client.localization.AlgoConstants'/>

    <gwt:HTMLPanel addStyleNames="{res.algoCSS.dispInlBlk}">
        <gwt:Anchor text="{i18n.heapSort}" href="#sort:heap"
                    addStyleNames="{res.algoCSS.shortcut}"/>
        <gwt:Anchor text="{i18n.quickSort}" href="#sort:quick"
                    addStyleNames="{res.algoCSS.shortcut}"/>
        <gwt:Anchor text="{i18n.mergeSort}" href="#sort:merge"
                    addStyleNames="{res.algoCSS.shortcut}"/>
        <gwt:Anchor text="{i18n.jungSort}" href="#sort:jung"
                    addStyleNames="{res.algoCSS.shortcut}"/>
    </gwt:HTMLPanel>

</ui:UiBinder>