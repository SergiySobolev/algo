<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui">

    <ui:with field='css' type="com.sbk.algo.client.resources.AlgoResources"/>
    <ui:with field='i18n' type='com.sbk.algo.client.localization.AlgoConstants'/>

    <gwt:HTMLPanel addStyleNames="{css.algoCSS.header}">
        <gwt:Anchor text="{i18n.algorithms}" href="#sorting:"
                    addStyleNames="{css.algoCSS.shadowed} {css.algoCSS.logo}"/>

        <gwt:HTMLPanel addStyleNames="{css.algoCSS.login}">
            <gwt:Anchor text="{i18n.registration}" href="#registration:"/>
        </gwt:HTMLPanel>

        <gwt:HTMLPanel addStyleNames="{css.algoCSS.marginLeft15} {css.algoCSS.login}">
            <gwt:Anchor text="{i18n.login}" href="#login:"/>
        </gwt:HTMLPanel>

        <gwt:HTMLPanel addStyleNames="{css.algoCSS.topSearch} {css.algoCSS.shadowed}">
            <gwt:TextBox addStyleNames="{css.algoCSS.formControl} {css.algoCSS.shadowed}"
                         ui:field="searchBox"/>
        </gwt:HTMLPanel>
    </gwt:HTMLPanel>

</ui:UiBinder>