<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
        >

    <ui:with field='res' type="com.sbk.core.client.resources.CoreResources"/>
    <ui:with field='local' type="com.sbk.core.client.localization.CoreConstants"/>

    <gwt:HorizontalPanel addStyleNames="{res.coreCSS.paddingTopBot5}">
        <gwt:Label ui:field="label" width="100px"/>
        <gwt:TextBox ui:field="textBox"/>
    </gwt:HorizontalPanel>

</ui:UiBinder>